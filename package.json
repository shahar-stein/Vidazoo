{
  "name": "react-express-app",
  "version": "0.1.0",
  "description": "Awesome and super-simple app with React front-end and Express back-end.",
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "https://github.com/alexdevero/react-express-app",
    "type": "git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [
    "app",
    "boilerplate",
    "express",
    "react",
    "starter"
  ],
  "bugs": {
    "url": "https://github.com/alexdevero/react-express-app/issues"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "concurrently \"yarn start-frontend || npm run start-frontend\" \"yarn start-backend || npm run start-backend\"",
    "start-backend": "nodemon server/server.js",
    "start-frontend": "react-scripts start",
    "build": "node ./buildScript",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.5.0",
    "@mui/material": "^5.3.0",
    "axios": "^1.4.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "debug": "~4.1.1",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "fs-extra": "8.1.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  }
}
